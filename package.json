{
  "name": "cms",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "concurrently": "^6.2.1"
  },
  "scripts": {
    "start": "concurrently 'npm start --prefix ./backend' 'npm start --prefix ./frontend'",
    "backend-build": "npm run build --workspace=backend",
    "frontent-build": "npm run build --workspace=frontend",
    "build": "npm install && npm run frontend-build"  
  },
  "workspaces": [
    "./backend/*",
    "./frontend/*"
  ],
  "engines": {
    "node": "16.10.x",
    "npm": "7.24.x"
  }
}
